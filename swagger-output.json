{
  "swagger": "2.0",
  "info": {
    "title": "Contacts API",
    "description": "Description",
    "version": "1.0.0"
  },
  "host": "cse341-project-2.onrender.com",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    },
    "api_key": {
      "type": "apiKey",
      "name": "api_key",
      "in": "header"
    },
    "oauth": {
      "type": "oauth2",
      "authorizationUrl": "http://api.example.com/api/auth/",
      "flow": "implicit",
      "scopes": {
        "read:players": "read player data"
      }
    }
  },
  "paths": {
    "/user/": {
      "get": {
        "tags":["get user"],
        "summary":"get all users",
        "description": "This is for when admin wants to look all infomation about users",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "tags":["change user db"],
        "summary":"create new user",
        "description": "This is for when user create an account",
        "parameters": [
          {
            "name": "user",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "example": "Sarah"
                },
                "lastName": {
                  "example": "Carter"
                },
                "userName": {
                  "example": "sarcar"
                },
                "password": {
                  "example": "zxcvb"
                },
                "email": {
                  "example": "sarcar@example.com"
                },
                "phoneNumber": {
                  "example": "444-444-444"
                },
                "country": {
                  "example": "USA"
                },
                "goals":{
                  "example": [{
                    "goalName":"go to the mission",
                    "startDate":"08-01-2023",
                    "endDate":"02-01-2025",
                    "plan":"prepare my documents"
                  }] 
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Bad Request"
          },
          "412": {
            "description": "Precondition Failed"
          }
        }
      }
    },
    "/user/{id}": {
      "get": {
        "tags":["get user"],
        "description": "This is for when admin wants to look infomation about a specific user",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      },
      "put": {
        "tags":["change user db"],
        "summary":"edit user information",
        "description": "This is for when user wants to update information",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          },{
            "name": "contact",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "example": "Sarah"
                },
                "lastName": {
                  "example": "Carter"
                },
                "userName": {
                  "example": "sarcar"
                },
                "password": {
                  "example": "zxcvb"
                },
                "email": {
                  "example": "sarcar@example.com"
                },
                "phoneNumber": {
                  "example": "444-444-444"
                },
                "country": {
                  "example": "USA"
                },
                "goals":{
                  "example": [{
                    "goalName":"go to the mission",
                    "startDate":"08-01-2023",
                    "endDate":"02-01-2025",
                    "plan":"prepare my documents"
                  }] 
                }
              }
            }
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "400": {
            "description": "Bad Request"
          },
          "412": {
            "description": "Precondition Failed"
          }
        }
      },
      "delete": {
        "tags":["change user db"],
        "summary":"delete user information",
        "description": "This is for when admin or user wants to information",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    }
  },
  "security": [
    {
      "Bearer": []
    }
  ]
}